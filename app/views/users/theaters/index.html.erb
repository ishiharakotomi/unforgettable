<% @theaters.each do |t|%>

  <%= link_to users_theater_path(t) do %>
    <%= attachment_image_tag t, :theater_image, fallback: "noimage.jpg" %>
    <p> 劇場名： <%= t.theater_name %></p>
  <% end %>
    <p> 劇場紹介： <%= t.introduction %></p>
    <p> 郵便番号： <%= t.postcode %></p>
    <p> 都道府県： <%= t.prefecture_name %></p>
    <p> 市区： <%= t.address_city %></p>
    <p> 町村： <%= t.address_street %></p>
    <p> 建物名： <%= t.address_building %></p>
    <p> 電話番号： <%= t.phonenumber %></p>

<% end %>